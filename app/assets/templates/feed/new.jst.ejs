<div class="row text-center">
	<div class="col-md-6 col-md-offset-3">
		<h1>Create New Feed</h1>

		<form id="new-feed-form">

			<div class="form-group">
			  <label for="title">Title</label>
			  <input type="text" id="title" class="form-control" name="feed[title]">
			</div>

			<div class="form-group"> 
			  <label for="url">URL</label>
			  <input type="text" id="url" class="form-control" name="feed[url]">
			</div>
			<br>
		  <input type="submit" value="Create Feed" class="btn btn-primary">

		</form>
	</div>
</div>